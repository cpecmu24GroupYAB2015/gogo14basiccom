to main
	set modea 0
	set rttt 0
	forever
	[
		set cans (summer 1 0 0 0 0 0)
		
		
		if(inbtw sensor7 574 578)[show "G  1" a, on b, off wait 6 set modea 1]
		if(inbtw sensor7 459 459)[show "G  2" a, off b, on wait 6]
		if(inbtw sensor7 326 342)[show "G  3" a, on b, on wait 6]
		if(inbtw sensor7 142 152)[show "defl" a, off b, off wait 6]
		if (modea = 0) [show "hole" wait 5]

		if(modea = 1)[set p1 cal		set p2 cal		set p3 cal		set p4 cal		set p5 cal		set p6 cal
			if (rttt  < 2) 
			[
				show cans	
				wait 30 
				show "read" 
				wait 10
				set rttt rttt+1
				calTime 1 1 (summer 1 0 0 0 0 0)
			]
			if( rta > 2 and rta = 2)[
				show "eee"
				wait 10
				set rttt 0
				set modea 0
			]
		]
	]
end



to cal
	output ((random 1) % 2)
end

to summer :p1 :p2 :p3 :p4 :p5 :p6
	output p1*1+p2*2+p3*4+p4*8+p5*16+p6*32
end

to calTime :time :qs :ans
	set t 0
	set t time*10
	set send 0
	set count 0
	repeat time *10
	[
		if(sensor8 > 500)[show "send" if(qs = ans)[show "rra" output 3 stop] wait 10]
		set t t-1
		show t
		wait 1		
	]
	show "out "
	repeat 30
	[
		beep
	]
	wait 50 
end

to inbtw :valuea :minaa :maxaa
	set outa 0
	if(valuea < 10)[	set outa 0]
	if((valuea > minaa-10) and (valuea < maxaa+10))[set outa 1]
	output outa
end
